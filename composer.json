{
    "name": "phlexible/frontend-search-bundle",
    "description": "phlexible frontend search bundle",
    "license": "proprietary",
	"type": "symfony-bundle",
	"keywords": ["indexer", "search"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\FrontendSearchBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.0",
        "phlexible/indexer-element-bundle": "*",
        "white-october/pagerfanta-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.brainbits.net/phlexible-bundles/",
            "options": {
                "ssl": {
                    "allow_self_signed": "true"
                }
            }
        }
    ],
    "minimum-stability": "stable"
}
